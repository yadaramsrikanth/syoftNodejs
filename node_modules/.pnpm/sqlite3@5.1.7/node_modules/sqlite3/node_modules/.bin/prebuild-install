#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/prebuild-install@7.1.2/node_modules/prebuild-install/bin.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/prebuild-install@7.1.2/node_modules/prebuild-install/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/prebuild-install@7.1.2/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/sqlite3@5.1.7/node_modules/sqlite3/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/sqlite3@5.1.7/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prebuild-install@7.1.2/node_modules/prebuild-install/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../prebuild-install@7.1.2/node_modules/prebuild-install/bin.js" "$@"
fi
