#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/@npmcli/move-file@1.1.2/node_modules/@npmcli/move-file/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/@npmcli/move-file@1.1.2/node_modules/@npmcli/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/@npmcli/move-file@1.1.2/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/@npmcli/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
