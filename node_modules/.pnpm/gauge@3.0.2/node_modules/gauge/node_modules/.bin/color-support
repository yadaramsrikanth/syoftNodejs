#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/color-support@1.1.3/node_modules/color-support/bin.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/color-support@1.1.3/node_modules/color-support/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/color-support@1.1.3/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/gauge@3.0.2/node_modules/gauge/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/gauge@3.0.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../color-support@1.1.3/node_modules/color-support/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../color-support@1.1.3/node_modules/color-support/bin.js" "$@"
fi
