#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/nopt.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/nopt@5.0.0/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Express-JS/syoftNodeJs/node_modules/.pnpm/node-gyp@8.4.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@5.0.0/node_modules/nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../../../../../nopt@5.0.0/node_modules/nopt/bin/nopt.js" "$@"
fi
